<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="sequence_employee_clearance" model="ir.sequence">
        <field name="name">Employee Clearance</field>
        <field name="code">employee.clearance</field>
        <field name="prefix">STBL/HR-%(range_month)s/%(range_year)s/</field>
        <field eval="1" name="number_next"/>
        <field eval="1" name="number_increment"/>
        <field eval="False" name="use_date_range"/>
        <field eval="False" name="company_id"/>
        <field name="padding">4</field>
    </record>

</odoo>